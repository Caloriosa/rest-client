{
  "name": "@caloriosa/rest-client",
  "version": "0.2.0",
  "description": "Caloriosa lightweight REST client for node.js/JavaScript",
  "main": "dist/caloriosa-restc.js",
  "module": "src/index.js",
  "scripts": {
    "build": "parallel-webpack",
    "test": "npm run lint && nyc ava --verbose",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "generate-docs": "jsdoc -c ./jsdoc.json",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Caloriosa/rest-client.git"
  },
  "keywords": [
    "client",
    "rest",
    "api",
    "http",
    "caloriosa",
    "component"
  ],
  "author": "Caloriosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Caloriosa/rest-client/issues"
  },
  "homepage": "https://github.com/Caloriosa/rest-client#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "codecov": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "docdash": "^0.4.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdoc": "^3.5.5",
    "nock": "^9.1.6",
    "node-mocks-http": "^1.6.6",
    "nyc": "^11.4.1",
    "parallel-webpack": "^2.2.0",
    "tui-jsdoc-template": "^1.2.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "ava": {
    "files": [
      "tests/unit/**/*.test.js",
      "tests/integration/**/*.test.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "powerAssert": false
  }
}
